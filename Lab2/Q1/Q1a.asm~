;Store data in Bank 0. Write a program to transfer the data from Bank 0 to Bank 1 using
;direct addressing mode.

mov r0,#4bh
mov r1,#61h
mov r2,#73h
mov r3,#68h
mov r4,#69h
mov r5,#73h
mov r6,#68h
mov r7,#47h

setb psw.3

mov r0,00h
mov r1,01h
mov r2,02h
mov r3,03h
mov r4,04h
mov r5,05h
mov r6,06h
mov r7,07h


end

